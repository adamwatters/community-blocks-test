: linear-gradient(\n      180deg,\n      #7286a7 0,\n      #7286a7 13%,\n      #fff 50%,\n      #812f30 56%,\n      #fff\n    );\n    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=\"#7286a7\",endColorstr=\"#ffffff\",GradientType=0);\n    background-clip: text;\n    -webkit-background-clip: text;\n    -webkit-text-fill-color: transparent;\n  }\n  .wordart.horizon .text:before {\n    content: attr(data-text);\n    position: absolute;\n    z-index: -1;\n    text-shadow: 0.01em 0 0 #161616, 0 0.01em 0 #8d8d8d, 0.02em 0.01em 0 #161616,\n      0.01em 0.02em 0 #8d8d8d, 0.03em 0.02em 0 #161616, 0.02em 0.03em 0 #8d8d8d,\n      0.04em 0.03em 0 #161616, 0.03em 0.04em 0 #8d8d8d, 0.05em 0.04em 0 #161616,\n      0.04em 0.05em 0 #8d8d8d, 0.06em 0.05em 0 #161616, 0.05em 0.06em 0 #8d8d8d,\n      0.07em 0.06em 0 #161616, 0.06em 0.07em 0 #8d8d8d, 0.08em 0.07em 0 #161616,\n      0.07em 0.08em 0 #8d8d8d, 0.09em 0.08em 0 #161616, 0.08em 0.09em 0 #8d8d8d,\n      0.1em 0.09em 0 #161616, 0.09em 0.1em 0 #8d8d8d;\n  }\n  .wordart.italicOutline {\n    transform: scaleY(1.3);\n    -webkit-transform: scaleY(1.3);\n    -moz-transform: scaleY(1.3);\n    -o-transform: scaleY(1.3);\n    -ms-transform: scaleY(1.3);\n  }\n  .wordart.italicOutline .text {\n    letter-spacing: -0.01em;\n    font-family: Arial, sans-serif;\n    font-weight: 700;\n    font-style: italic;\n    color: #fff;\n    -webkit-text-stroke: 0.01em #000;\n    filter: progid:DXImageTransform.Microsoft.Glow(Color=#000000,Strength=1);\n    text-shadow: 0.03em 0.03em 0 #6d6d6d;\n  }\n  .wordart.slate {\n    transform: scaleY(1.5);\n    -webkit-transform: scaleY(1.5);\n    -moz-transform: scaleY(1.5);\n    -o-transform: scaleY(1.5);\n    -ms-transform: scaleY(1.5);\n  }\n  .wordart.slate .text {\n    font-family: Times, Times New Roman, serif;\n    font-weight: 400;\n    color: #2f5485;\n    text-shadow: 0.03em 0.03em 0 #b3b3b3;\n  }\n\n  #root {\n    transition: opacity 1s;\n    position: absolute;\n    top: 0;\n    right: 0;\n    bottom: 0;\n    left: 0;\n    opacity: 1;\n    overflow: hidden;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    font-size: 4vw;\n  }\n\n  #root.hidden {\n    opacity: 0;\n  }\n<\/style>\n\n<div class=\"hidden\" id=\"root\"><\/div>\n\n<script type=\"text\/javascript\">\n  document.addEventListener(\"DOMContentLoaded\", function() {\n    var root = document.getElementById(\"root\");\n    var container = document.createElement(\"DIV\");\n    var text = document.createElement(\"DIV\");\n    container.classList.add(\"wordart\");\n    container.classList.add(window.style || \"blues\");\n    text.classList.add(\"text\");\n    text.innerHTML = window.text || 'Set \"window.text\"';\n    container.appendChild(text);\n    root.append(container);\n    var style = document.createElement(\"style\");\n    style.innerHTML = `\n      #root {\n        font-size: ${Number(window.fontSize) || 3}vw;\n      }\n      .text {\n          text-align: ${window.textAlign || \"center\"};\n      }\n    `;\n    document.head.appendChild(style);\n    \/\/ everything is in place, show the codeblock\n    document.getElementById(\"root\").classList.remove(\"hidden\");\n  });\n<\/script>",
