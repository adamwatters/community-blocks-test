        "code" : "<script>\n\n\/\/ WordArt Code Block\n\ntext = \"{{TEXT}}\";\nstyle = \"{{STYLE}}\";\nfontSize = 5;\ntextAlign = \"center\";\n\n\/\/style options include \"rainbow\", \"blues\", \"superhero\", \"radial\", \"tilt\", \"purple\", \"horizon\", \"italicOutline\", and \"slate\"\n\n\/\/ textAlign options include \"left\", \"right\", \"center\"\n\n\n<\/script>\n<style>\n  @charset \"UTF-8\";\n  .wordart {\n    font-family: Arial, sans-serif;\n    font-size: 4em;\n    font-weight: 700;\n    position: relative;\n    z-index: 1;\n    display: inline-flex;\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n  }\n  .wordart.rainbow {\n    transform: scaleY(1.5);\n    -webkit-transform: scaleY(1.5);\n    -moz-transform: scaleY(1.5);\n    -o-transform: scaleY(1.5);\n    -ms-transform: scaleY(1.5);\n  }\n  .wordart.rainbow .text {\n    background: red;\n    background: -webkit-linear-gradient(\n      left,\n      #b306a9,\n      #ef2667,\n      #f42e2c,\n      #ffa509,\n      #fdfc00,\n      #55ac2f,\n      #0b13fd,\n      #a804af\n    );\n    background: -o-linear-gradient(\n      left,\n      #b306a9,\n      #ef2667,\n      #f42e2c,\n      #ffa509,\n      #fdfc00,\n      #55ac2f,\n      #0b13fd,\n      #a804af\n    );\n    background: -webkit-gradient(\n      linear,\n      left top,\n      right top,\n      from(#b306a9),\n      color-stop(#ef2667),\n      color-stop(#f42e2c),\n      color-stop(#ffa509),\n      color-stop(#fdfc00),\n      color-stop(#55ac2f),\n      color-stop(#0b13fd),\n      to(#a804af)\n    );\n    background: linear-gradient(\n      90deg,\n      #b306a9,\n      #ef2667,\n      #f42e2c,\n      #ffa509,\n      #fdfc00,\n      #55ac2f,\n      #0b13fd,\n      #a804af\n    );\n    -webkit-background-clip: text;\n    -webkit-text-fill-color: transparent;\n  }\n  .wordart.blues .text {\n    font-family: Impact, sans-serif;\n    color: #24c0fd;\n    -webkit-text-stroke: 0.01em #00a;\n    filter: progid:DXImageTransfo